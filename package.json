{
  "name": "rigtch-music-api",
  "version": "0.0.1",
  "description": "",
  "author": "Rigtch",
  "license": "Apache License 2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docker:dev": "docker-compose up",
    "docker:build": "docker-compose up --build -V",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/auth/test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./apps/auth/test/jest-e2e.json --watch",
    "stryker": "stryker run",
    "prepare": "husky install"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@nestjs/apollo": "10.1.4",
    "@nestjs/axios": "1.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "10.1.5",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/microservices": "9.1.6",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "amqp-connection-manager": "4.1.9",
    "amqplib": "0.10.3",
    "apollo-server-core": "3.11.0",
    "apollo-server-express": "3.11.0",
    "axios": "1.1.3",
    "graphql": "16.6.0",
    "joi": "17.7.0",
    "pactum": "3.2.4",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "passport-spotify": "2.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@commitlint/types": "^17.0.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@stryker-mutator/core": "6.3.0",
    "@stryker-mutator/jest-runner": "6.3.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "3.0.7",
    "@types/passport-spotify": "2.0.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.16.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "husky": "^8.0.1",
    "jest": "28.1.3",
    "lint-staged": "^13.0.3",
    "node-mocks-http": "1.11.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  }
}
